<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox spacing="20" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.chat.ui.ErrorDialog">
    <padding><Insets top="20" right="20" bottom="20" left="20"/></padding>
    <Label fx:id="messageLabel" wrapText="true" style="-fx-text-fill: #e74c3c; -fx-font-size: 14;" />
    <Button text="确定" onAction="#closeDialog" defaultButton="true"
            style="-fx-background-color: #e74c3c; -fx-text-fill: white;" />
</VBox>